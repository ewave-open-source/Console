{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "target": "es5",
    "sourceMap": true
  },

  "exclude": [
    "server/node_modules",
    "client/node_modules",
    "node_modules",
    "bower_components",
    "noduls",
    "wwwroot",
    "release"
      
  ]
}